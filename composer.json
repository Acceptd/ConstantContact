{
    "name": "acceptd/constant-contact",
    "description": "Constant Contact SDK",
    "keywords": [
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://getacceptd.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Bill Condo",
            "homepage": "https://billcondo.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "OpenAPI\\Client\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "OpenAPI\\Client\\Test\\" : "test/" }
    }
}
